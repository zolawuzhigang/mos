{
  "mcpServers": {
    "mcp-deepwiki": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "mcp-deepwiki@latest"
      ]
    },
    "trends-hub": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "mcp-trends-hub@1.6.0"
      ]
    },
    "arxiv-mcp-server": {
      "command": "uv",
      "args": [
        "tool",
        "run",
        "arxiv-mcp-server",
        "--storage-path",
        "./paper_storage"
      ]
    },
    "pozansky-stock-server": {
      "command": "uvx",
      "args": [
        "pozansky-stock-server"
      ]
    },
    "worldbank-mcp": {
      "command": "npx.cmd",
      "args": [
        "worldbank-mcp"
      ],
      "description": "World Bank Open Data MCP Server",
      "name": "World Bank Data"
    },
    "mcp-server-hotnews": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "@wopal/mcp-server-hotnews"
      ]
    },
    "web-search": {
      "command": "npx.cmd",
      "args": [
        "open-websearch@1.1.5"
      ]
    },
    "biomcp": {
      "command": "uv",
      "args": [
        "run",
        "--with",
        "biomcp-python",
        "biomcp",
        "run"
      ]
    },
    "searxng": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "mcp-searxng"
      ],
      "env": {
        "SEARXNG_URL": "https://searx.stream"
      }
    },
    "bing-search": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "bing-cn-mcp"
      ]
    }
  }
}